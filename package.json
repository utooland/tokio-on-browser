{
  "name": "tokio-on-browser",
  "scripts": {
    "install-toolchain": "cargo install wasm-bindgen-cli && brew install binaryen",
    "build-wasm": "cargo build --target wasm32-unknown-unknown -Z build-std=panic_abort,std",
    "bindgen-dev": "wasm-bindgen target/wasm32-unknown-unknown/wasm-dev/tokio_on_browser.wasm --out-dir js/wasm --out-name index --target web --debug --keep-debug",
    "dev": "rm -rf js/wasm && npm run build-wasm -- --profile wasm-dev && npm run bindgen-dev",
    "start": "rm -rf dist && webpack serve"
  },
  "devDependencies": {
    "html-webpack-plugin": "^5.6.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.2"
  }
}
